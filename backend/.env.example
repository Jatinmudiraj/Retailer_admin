# Backend
ENV=dev
BACKEND_URL=http://localhost:9001
FRONTEND_ORIGIN=http://localhost:5173

# IMPORTANT: Put your NEW password/user here (do not commit .env)
# Cockroach example (note sslrootcert is important for verify-full on many clients):
# DATABASE_URL="postgresql+psycopg://retailer_admin_rw:REDACTED@your-host:26257/royaliq?sslmode=verify-full&sslrootcert=/home/you/.cockroach-certs/root.crt"
DATABASE_URL="postgresql+psycopg://USER:PASSWORD@HOST:26257/royaliq?sslmode=verify-full&sslrootcert=/ABS/PATH/root.crt"

# Google Identity client id (OAuth Web client id)
GOOGLE_CLIENT_ID="YOUR_GOOGLE_CLIENT_ID.apps.googleusercontent.com"

# JWT for admin session cookie signing
JWT_SECRET="change_this_to_a_long_random_secret"
COOKIE_SECURE=0

# Admin access control
# Comma-separated allowed emails OR domains
ADMIN_EMAILS="youradmin@gmail.com"
ADMIN_DOMAINS=""

# Media storage (uploaded images)
MEDIA_DIR="./media"

# App settings
GOLD_RATE_PER_GRAM=6500
